  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "Bearer",
        "type": "http",
        "name": "JWT",
        "description": "Access Token",
        "in": "header"
      }
    },
    "schemas": {
      "SocialProviderName": {
        "type": "string",
        "enum": [
          "facebook"
        ]
      },
      "SocialRequestBody": {
        "type": "object",
        "properties": {
          "provider": {
            "$ref": "#/components/schemas/SocialProviderName"
          }
        },
        "required": [
          "provider"
        ]
      },
      "SocialAuthUrlResponse": {
        "type": "object",
        "properties": {
          "authUrl": {
            "type": "string"
          },
          "nonce": {
            "type": "string"
          }
        },
        "required": [
          "authUrl",
          "nonce"
        ]
      },
      "SocialValidateBody": {
        "type": "object",
        "properties": {
          "provider": {
            "$ref": "#/components/schemas/SocialProviderName"
          },
          "idToken": {
            "type": "string"
          },
          "accessToken": {
            "type": "string"
          },
          "nonce": {
            "type": "string"
          }
        },
        "required": [
          "provider"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "emailVerified": {
            "type": "boolean"
          },
          "profile": {
            "type": "object"
          }
        },
        "required": [
          "id",
          "email",
          "emailVerified",
          "profile"
        ]
      },
      "SocialProfileName": {
        "type": "object",
        "properties": {
          "givenName": {
            "type": "string"
          },
          "familyName": {
            "type": "string"
          }
        },
        "required": [
          "givenName",
          "familyName"
        ]
      },
      "SocialProfile": {
        "type": "object",
        "properties": {
          "provider": {
            "type": "string"
          },
          "providerUserId": {
            "type": "string"
          },
          "accessToken": {
            "type": "string"
          },
          "name": {
            "$ref": "#/components/schemas/SocialProfileName"
          },
          "email": {
            "type": "string"
          },
          "emailVerified": {
            "type": "boolean"
          }
        },
        "required": [
          "provider",
          "providerUserId",
          "name",
          "email",
          "emailVerified"
        ]
      },
      "SocialAuthenticationResponse": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "socialProfile": {
            "$ref": "#/components/schemas/SocialProfile"
          },
          "providerTokenClaims": {
            "type": "object"
          }
        },
        "required": [
          "accessToken",
          "user",
          "socialProfile",
          "providerTokenClaims"
        ]
      },
      "UserSignupBody": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "profile": {
            "type": "object"
          }
        },
        "required": [
          "email",
          "password",
          "profile"
        ]
      },
      "AuthenticationResponse": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "required": [
          "accessToken",
          "user"
        ]
      },
