{
  "name": "caliobase-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/caliobase-ui/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/caliobase-ui",
        "tsConfig": "packages/caliobase-ui/tsconfig.lib.json",
        "project": "packages/caliobase-ui/package.json",
        "entryFile": "packages/caliobase-ui/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/caliobase-ui/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/caliobase-ui"],
      "options": {
        "jestConfig": "packages/caliobase-ui/jest.config.ts"
      }
    }
  }
}
