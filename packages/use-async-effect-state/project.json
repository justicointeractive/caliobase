{
  "name": "use-async-effect-state",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/use-async-effect-state/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/use-async-effect-state",
        "tsConfig": "packages/use-async-effect-state/tsconfig.lib.json",
        "project": "packages/use-async-effect-state/package.json",
        "entryFile": "packages/use-async-effect-state/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/use-async-effect-state/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/use-async-effect-state"],
      "options": {
        "jestConfig": "packages/use-async-effect-state/jest.config.ts"
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "trackDeps": true,
        "postTargets": ["use-async-effect-state:deploy"]
      }
    }
  }
}
