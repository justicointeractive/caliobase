    bucket: optional('S3_BUCKET', String, ''),
    keyPrefix: optional('S3_KEY_PREFIX', String, ''),
    endpoint: optional('S3_ENDPOINT', String, undefined),
  },
  cdn: {
    urlPrefix: optional('STATIC_FILE_BASEURL', String, ''),
  },
}));

