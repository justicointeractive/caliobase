{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "caliobase",
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "examples"
  },
  "targetDependencies": {
    "version": [
      {
        "target": "version",
        "projects": "dependencies"
      }
    ],
    "serve": [{ "target": "build", "projects": "self" }],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "implicitDependencies": {
    "package.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "MTMyODlkOTctOTU5Yi00NzkwLWE1NGEtNWFlMDg2MDY4MTIxfHJlYWQtd3JpdGU="
      }
    }
  },
  "defaultProject": "example"
